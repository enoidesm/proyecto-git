def main():

    print("factorial")

    número = int(input("digite un numero entero:"))

 

    if número <= 0:

        print("el número debe ser mayor que 0")

    else:

        factorial = 1

        for i in range(1, número + 1):

            factorial = factorial * i

        print(f"El factorial de {número} es {factorial}.")

 

if __name__ == "__main__":

    main()